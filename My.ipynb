    "#Рабочий варик\n",
    "import random\n",
    
    "N = int (input ())\n",
    "N = list (map (int, input().split()))            \n",
    "def QuickSort(N):\n",
    "    if len(N) <= 2 or len(N) >= 10000:\n",
    "        return N\n",
    "    else:\n",
    "        q = random.choice(N)\n",
    "        L = []\n",
    "        M = []\n",
    "        R = []\n",
    "        Z = []\n",
    "        for elem in N:\n",
    "            if elem < q and elem%3 != 0:\n",
    "                L.append(elem)\n",
    "            elif elem > q and elem%3 != 0:\n",
    "                R.append(elem)\n",
    "            elif elem == q and elem%3 != 0:\n",
    "                M.append(elem)\n",
    "            else:\n",
    "                Z.append(elem)\n",
    "        return  sorted(Z) + QuickSort(L) + M + QuickSort(R)\n",
    "\n",
    "print (QuickSort(N))"
   ]
